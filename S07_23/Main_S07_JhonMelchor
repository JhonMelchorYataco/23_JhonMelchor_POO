        Estudiante_23 estudiante = new Estudiante_23("Juan", 20, "Informática");
        Calculadora_23 calculadora = new Calculadora_23();
        Animal_23 animal = new Animal_23("Gato");

        // Realizar operaciones o llamadas a métodos aquí
        estudiante.mostrarInformacion();
